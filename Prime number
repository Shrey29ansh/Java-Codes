class Prime{
    public static void main(String args[])
    {
        int n=4,r;
r=n%2;
        System.out.println("checking prime number or not");
        if(n==2)
        {
            System.out.println("prime number");
        }
        else if(r == 0)
        {
             System.out.println("Not a prime number");
        }
        else
        {
             System.out.println("Prime number");
        }
    }
}
