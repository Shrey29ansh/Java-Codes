public class fib{
      public static void main(String args[])
      {
         
          int n1=0,n2=1,sum,n=7;
          System.out.println("Fibonacci Series upto "+n+" is");
          for(int i=1;i<=n;i++)
          {
              System.out.println(n1+" ");
              sum=n1+n2;
              n1=n2;
              n2=sum;
          }
      }
}
